<% if object.errors.full_messages.length > 0 %>
  <div class="errors-wrapper">
    <h5>Your vote had the following errors:</h5>
    <ul>
      <% object.errors.messages.values.each do |msg| %>
        <% msg.each do  |m| %>
          <li><%= m %></li>
        <% end %>
      <% end %>
    </ul>
  </div>
<% end %>